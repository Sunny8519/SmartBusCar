<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="test.yang.com.smartbuscar">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:name=".App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/BaseAppTheme">
        <activity android:name=".module.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".module.activity.MainActivity" />
        <activity android:name=".module.activity.LoginActivity" />
        <activity android:name=".module.activity.RegisterActivity" />
        <activity android:name=".module.activity.DetailInfoActivity" />
        <activity android:name=".module.activity.RechargeActivity" />
        <activity android:name=".module.activity.ReportLossActivity" />
    </application>

</manifest>